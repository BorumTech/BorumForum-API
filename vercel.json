{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.5.2"
    }
  },
  "routes": [
    {
      "src": "/login",
      "dest": "/api/login.php"
    },
    {
      "src": "/register",
      "dest": "/api/register.php"
    },
    {
      "src": "/questions",
      "dest": "/api/questions.php"
    }
  ]
}